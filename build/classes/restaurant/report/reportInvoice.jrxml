	<queryString>
		<![CDATA[SELECT  
	dbo.DiningTables.TableName, 
	dbo.Employees.FullName, 
	dbo.Invoices.InvoiceID, 
	dbo.Invoices.Tax, 
	dbo.Invoices.Discount, 
	dbo.Invoices.PaymentTime, 
	dbo.Invoices.TotalAmount, 
	dbo.OrderDetails.ProductQuantity, 
	dbo.OrderDetails.Note, 
	dbo.Orders.CreatedDate, 
	dbo.Products.ProductName, 
	dbo.Products.Price, 
	dbo.Orders.Total
FROM      
	dbo.Invoices 
    LEFT JOIN dbo.Employees ON dbo.Invoices.EmployeeID = dbo.Employees.EmployeeID 
    LEFT JOIN dbo.Orders ON dbo.Invoices.InvoiceID = dbo.Orders.InvoiceID 
    LEFT JOIN dbo.DiningTables ON dbo.Orders.TableID = dbo.DiningTables.TableID 
    LEFT JOIN dbo.OrderDetails ON dbo.Orders.OrderID = dbo.OrderDetails.OrderID
    LEFT JOIN dbo.Products ON dbo.OrderDetails.ProductID = dbo.Products.ProductID
WHERE 
    dbo.Invoices.InvoiceID = $P{ToInvoiceID}]]>
	</queryString>
